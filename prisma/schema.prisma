// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

// model Snippet {
//   id    Int    @id @default(autoincrement())
//   title String
//   code  String
// }

model User {
  id       Int    @id @default(autoincrement())
  name     String
  password String
}

model Box {
  id    Int    @id @default(autoincrement())
  title String
  memo  String
}

model Nav {
  id    Int    @id @default(autoincrement())
  title String
  memo  String
  link  String
}
